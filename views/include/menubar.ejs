<div class="col-2">
    <table id="menubar" class="table table-hover table-info text-center" role="button">
        <% for(key in menubar){%>
            <tr>
                <td onclick="location.href = '<%=menubar[key]%>'">
                    <%=key%> &gt;&gt;
                </td>
            </tr>
        <%}%>
      </table>

</div>

<style>
    #menubar { 
        position:absolute; top:80px;
    }
</style>

<script>
$(window).scroll(function(){
    var scrollTop = $(document).scrollTop();
    if (scrollTop < 180) {
        scrollTop = 80;
    }
    $("#menubar").stop();
    $("#menubar").animate( { "top" : scrollTop });
});
</script>